/** * @author Balakrishna [vamsibalu@gmail.com] * @version 2.0 */package com.events{		import com.modal.PlayerDataVO;		import flash.events.Event;
		public class CustomEvent extends Event{				public static const DEFAULT_NAME:String = "CustomEvent";				// event constants		public static const REMOTEDATA_CHANGE:String = "dataChange";				public var data:PlayerDataVO;			public function CustomEvent($type:String,$data:PlayerDataVO, $bubbles:Boolean = false, $cancelable:Boolean = false){			super($type, $bubbles, $cancelable);			this.data = $data;		}			public override function clone():Event{			return new CustomEvent(type, data, bubbles, cancelable);		}				public override function toString():String{			return formatToString("CustomEvent", "data", "type", "bubbles", "cancelable");		}	}}